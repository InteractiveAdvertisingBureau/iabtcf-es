{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "preserveConstEnums": true,
    "resolveJsonModule": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "DOM"
    ]
  }
}
