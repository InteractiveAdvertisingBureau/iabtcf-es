    "vendor",
    "framework",
    "tcf",
    "lib",
    "library",
    "cmp",
    "2.0",
    "v2.0",
    "v2",
    "software",
    "development",
    "api",
    "kit",
    "sdk",
    "gdpr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/InteractiveAdvertisingBureau/iabtcf-es.git",
    "directory": "modules/core"
  },
  "dependencies": {}
